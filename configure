#!/bin/sh

echo Unnecessary in Debian branch: Makefile already patched.
exit

f=""
p=`echo $PATH | sed -e 's/:/ /g'`
for d in $p; do
	if [ -x $d/fortune ]; then
		f=$d/fortune
		break
	fi
done
if [ $f != "" ]; then break; fi

$f
echo
echo "Now edit the PREFIX variable of Makefile, then run 'make'"
echo
